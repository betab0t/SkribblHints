{
  "name": "SkribblHints",
  "version": "1.0",
  "description": "Chrome extension to help you win online Skribbl(http://skribbl.io) games!",
  "manifest_version": 2,
  "content_scripts": [{
      "js": ["inject.js"],
      "matches": ["https://skribbl.io/*"]
  }],
  "web_accessible_resources": ["script.js",
                               "words.json"]
}
